@model WebMedForms.Models.Solicitacao

@{
    ViewData["Title"] = "Edit";
}

<h1>Agendar</h1>

<hr />
<div class="row">
    <div class="col-md">
        <form asp-action="Edit">
            <input type="hidden" asp-for="Id" />
            <input type="hidden" asp-for="CodStatus" />
            <input type="hidden" asp-for="NomeMedicoSolicitante" />
            <input type="hidden" asp-for="CrmMedicoSolicitante" />
            <input type="hidden" asp-for="ChaveAutenticacao" />
            <input type="hidden" asp-for="NomePaciente" />
            <input type="hidden" asp-for="CPF" />
            <input type="hidden" asp-for="RG" />
            <input type="hidden" asp-for="DataNascimento" />
            <input type="hidden" asp-for="CadUnicoSaude" />
            <input type="hidden" asp-for="Telefone" />
            <input type="hidden" asp-for="Celular" />
            <input type="hidden" asp-for="Email" />
            <input type="hidden" asp-for="Endereco" />
            <input type="hidden" asp-for="Complemento" />
            <input type="hidden" asp-for="Cidade" />
            <input type="hidden" asp-for="Estado" />
            <input type="hidden" asp-for="CEP" />
            <input type="hidden" asp-for="CID" />
            <input type="hidden" asp-for="IndicacaoMedica" />
            <input type="hidden" asp-for="IndicacaoTratamento" />
            <input type="hidden" asp-for="Prioridade" />
            <div class="form-group">
                <label asp-for="DataAgendamento1" class="control-label"></label>
                <input asp-for="DataAgendamento1" class="form-control" />
                <span asp-validation-for="DataAgendamento1" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DataAgendamento2" class="control-label"></label>
                <input asp-for="DataAgendamento2" class="form-control" />
                <span asp-validation-for="DataAgendamento2" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DataAgendamento3" class="control-label"></label>
                <input asp-for="DataAgendamento3" class="form-control" />
                <span asp-validation-for="DataAgendamento3" class="text-danger"></span>
            </div>
            <br />
            <div>
                <input type="submit" value="Agendar Solicitação" class="btn btn-primary" /> |
                <a class="btn btn-primary" asp-action="Index">Voltar</a>
            </div>
        </form>
    </div>
</div>
<br />


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
